RUN echo '#!/bin/sh.real\n\
cat /.balena/image-info\n\
rm -f /bin/sh\n\
cp /bin/sh.real /bin/sh\n\
/bin/sh "$@"' > /bin/sh-shim \
	&& chmod +x /bin/sh-shim \
	&& cp /bin/sh /bin/sh.real \
	&& mv /bin/sh-shim /bin/sh